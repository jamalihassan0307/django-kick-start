{
  "name": "django-kick-start",
  "displayName": "Django Kick Start",
  "description": "Beginner-Focused Django Starter Toolkit for one-click setup",
  "version": "1.0.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "django",
    "python",
    "web",
    "framework",
    "starter"
  ],
  "icon": "media/logo.png",
  "galleryBanner": {
    "color": "#1c2834",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:django-kick-start.initProject"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "django-kick-start.initProject",
        "title": "Django: Initialize New Project",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.generateApp",
        "title": "Django: Generate New App"
      },
      {
        "command": "django-kick-start.staticHelper",
        "title": "Django: Setup Static Files"
      },
      {
        "command": "django-kick-start.templateScaff",
        "title": "Django: Scaffold Templates"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/django-kick-start"
  },
  "bugs": {
    "url": "https://github.com/yourusername/django-kick-start/issues"
  },
  "homepage": "https://github.com/yourusername/django-kick-start#readme",
  "license": "MIT"
}
