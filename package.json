{
  "name": "django-kick-start",
  "displayName": "Django Kick Start",
  "description": "A VS Code extension to kickstart Django development",
  "version": "1.0.4",
  "publisher": "yourusername",
  "engines": {
    "vscode": "^1.85.0",
    "node": "^16.0.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "django",
    "python",
    "web",
    "framework",
    "starter"
  ],
  "icon": "media/logo.png",
  "galleryBanner": {
    "color": "#1c2834",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "django-kick-start.initProject",
        "title": "Django: Initialize New Project",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.generateApp",
        "title": "Django: Generate New App",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.staticHelper",
        "title": "Django: Setup Static Files",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.templateScaff",
        "title": "Django: Scaffold Templates",
        "category": "Django Kick Start"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.2",
    "@vscode/test-electron": "^2.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/django-kick-start"
  },
  "bugs": {
    "url": "https://github.com/yourusername/django-kick-start/issues"
  },
  "homepage": "https://github.com/yourusername/django-kick-start#readme",
  "license": "MIT"
}
