{
  "name": "django-kick-start",
  "displayName": "Django Kick Start",
  "description": "A VS Code extension to kickstart Django development",
  "version": "1.1.2",
  "publisher": "jamalihassan0307",
  "engines": {
    "vscode": "^1.85.0",
    "node": "^16.0.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "django",
    "python",
    "web",
    "framework",
    "starter",
    "html",
    "template",
    "django-kick-start",
    "django-extension",
    "django-starter",
    "django-template",
    "django-html",
    "django-debug",
    "django-server",
    "django-development",
    "django-tools",
    "django-productivity",
    "django-snippets",
    "django-automation",
    "django-wizard",
    "django-scaffold",
    "django-boilerplate",
    "django-quickstart",
    "django-helper",
    "django-assistant",
    "django-ide",
    "django-vscode",
    "django-commands",
    "django-templates",
    "django-static",
    "django-media",
    "django-apps",
    "django-project",
    "django-setup",
    "django-configuration",
    "django-debugging",
    "django-server",
    "django-development",
    "django-workflow",
    "django-productivity",
    "django-automation",
    "django-tools",
    "django-utilities",
    "jamalihassan0307",
    "jamalihassan"
  ],
  "icon": "media/logo.png",
  "galleryBanner": {
    "color": "#1c2834",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "django-kick-start.initProject",
        "title": "Django: Initialize Project",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.generateApp",
        "title": "Django: Generate App",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.staticHelper",
        "title": "Django: Setup Static Files",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.templateScaff",
        "title": "Django: Generate Templates",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.createModel",
        "title": "Django: Create Model",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.createhtml",
        "title": "Django: Create HTML Template",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.createstatic",
        "title": "Django: Create Static File",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.runServer",
        "title": "Django: Run Development Server",
        "category": "Django Kick Start"
      },
      {
        "command": "django-kick-start.createView",
        "title": "Django: Create View and URL",
        "category": "Django Kick Start"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.2",
    "@vscode/test-electron": "^2.3.8"
  },
  "repository": {
    "type": "git", 
    "url": "https://github.com/jamalihassan0307/django-kick-start"
  },
  "bugs": {
    "url": "https://github.com/jamalihassan0307/django-kick-start/issues"
  },
  "homepage": "https://github.com/jamalihassan0307/django-kick-start#readme",
  "license": "MIT"
}
